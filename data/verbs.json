    "patterns": [
      {
        "type": "er_endings",
        "group": "er",
        "endings": {
          "present": {
            "je": "-e",
            "tu": "-es",
            "il/elle/on": "-e",
            "nous": "-ons",
            "vous": "-ez",
            "ils/elles": "-ent"
          }
        },
        "example": {
          "infinitive": "parler",
          "stem": "parl",
          "forms": {
            "present": {
              "je": "parle",
              "tu": "parles",
              "il": "parle",
              "elle": "parle",
              "on": "parle",
              "nous": "parlons",
              "vous": "parlez",
              "ils": "parlent",
              "elles": "parlent"
            }
          }
        },
        "grammar": {
          "present": {
            "grammar_eng": "Regular -ER verbs: stem = infinitive minus -er; add the present endings. Note: je → j’ before a vowel or mute h (e.g., j’aime).",
            "grammar_ru": "Правильные -ER: основа = инфинитив без -er; добавляем окончания настоящего времени. Примечание: je → j’ перед гласной или немой h (например, j’aime)."
          }
        }
      },
      {
        "type": "ir_endings",
        "group": "ir",
        "endings": {
          "present": {
            "je": "-is",
            "tu": "-is",
            "il/elle/on": "-it",
            "nous": "-issons",
            "vous": "-issez",
            "ils/elles": "-issent"
          }
        },
        "example": {
          "infinitive": "finir",
          "stem": "fin",
          "forms": {
            "present": {
              "je": "finis",
              "tu": "finis",
              "il": "finit",
              "elle": "finit",
              "on": "finit",
              "nous": "finissons",
              "vous": "finissez",
              "ils": "finissent",
              "elles": "finissent"
            }
          }
        },
        "grammar": {
          "present": {
            "grammar_eng": "Regular -IR (finir-type) verbs: stem = infinitive minus -ir; add present endings (-is, -is, -it, -issons, -issez, -issent).",
            "grammar_ru": "Правильные -IR (тип finir): основа = инфинитив без -ir; добавляем окончания настоящего времени (-is, -is, -it, -issons, -issez, -issent)."
          }
        }
      },
      {
        "type": "re_endings",
        "group": "re",
        "endings": {
          "present": {
            "je": "-s",
            "tu": "-s",
            "il/elle/on": "no ending",
            "nous": "-ons",
            "vous": "-ez",
            "ils/elles": "-ent"
